<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE MEEGO_IM_TOOLBAR_WIDGET SYSTEM 'VirtualKeyboardToolbarDTD.dtd'>
<input-method version="1">
    <toolbar>
        <items>
            <button name="emoticon" group="emoticon" showon="always" alignment="left" text="" text_id="" icon_id="icon-m-messaging-smiley-happy" toggle="false" pressed="false">
                <actions>
                    <sendkeysequence keysequence="Meta+j"> </sendkeysequence>
                </actions>
            </button>
            <button name="Up" group="Up" showon="always" text="&#x2b06;" toggle="false" pressed="false">
                <actions>
                    <sendkeysequence keysequence="Up"> </sendkeysequence>
                </actions>
            </button>
            <button name="Down" group="Down" showon="always" text="&#x2b07;" toggle="false" pressed="false">
                <actions>
                    <sendkeysequence keysequence="Down"> </sendkeysequence>
                </actions>
            </button>
            <button name="Left" group="Left" showon="always" text="&#x2b05;" toggle="false" pressed="false">
                <actions>
                    <sendkeysequence keysequence="Left"> </sendkeysequence>
                </actions>
            </button>
            <button name="Right" group="Right" showon="always" text="&#x27a1;" toggle="false" pressed="false">
                <actions>
                    <sendkeysequence keysequence="Right"> </sendkeysequence>
                </actions>
            </button>
            <button name="send" group="send" showon="always" alignment="right" text="" text_id="qtn_comm_send" icon_id="" toggle="false" pressed="false" enabled="false" highlighted="true">
                <actions>
                    <sendkeysequence keysequence="Meta+l"> </sendkeysequence>
                </actions>
            </button>
        </items>
        <layout>
            <item name="emoticon" />
            <item name="Up" />
            <item name="Down" />
            <item name="Left" />
            <item name="Right" />
            <item name="send" />
        </layout>
    </toolbar>
</input-method>
